{
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        },
        "experimentalDecorators": true
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
     },
    "rules": {
        "linebreak-style": 0,
        "object-curly-spacing": ["warn", "always"]
    }
}
